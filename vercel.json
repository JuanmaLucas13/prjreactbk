{
  "version": 2,
  "builds": [
      {
          "src": "./index.py",
          "use": "@vercel/node"
      }
  ],
  "rewrites": [
      { 
        "source": "/(.*)", 
        "destination": "./index.js" 
      }
  ],
  "headers": 
  [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      }
  ]
}